package com.dev.search;

public class BinarySearch {
	
	static public boolean binarySearch(int[] arr, int key) {
		int start = 0;
		int end = arr.length-1;
		while(start <= end) {
			int mid = start + (end-start)/2;
			if(arr[mid] == key) {
				return true;
			}
			if(arr[mid] > key) {
				end = mid - 1;
			}
			else {
				start = mid + 1;
			}
		}
		return false;
	}

}
